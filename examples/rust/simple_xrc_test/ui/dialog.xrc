<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resource xmlns="http://www.wxwidgets.org/wxxrc" version="2.5.3.0">
  <object class="wxDialog" name="test_dialog">
    <size>400,300</size>
    <style>wxDEFAULT_DIALOG_STYLE|wxRESIZE_BORDER</style>
    <title>XRC Dialog Example</title>
    <centered>1</centered>
    <object class="wxBoxSizer" name="">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <flag>wxEXPAND|wxALL</flag>
        <border>10</border>
        <option>0</option>
        <object class="wxStaticText" name="dialog_title">
          <font>
            <size>16</size>
            <style>normal</style>
            <weight>bold</weight>
            <family>default</family>
            <underlined>0</underlined>
          </font>
          <label>Dialog from XRC</label>
          <wrap>0</wrap>
        </object>
      </object>
      <object class="sizeritem">
        <flag>wxEXPAND|wxALL</flag>
        <border>10</border>
        <option>0</option>
        <object class="wxPanel" name="dialog_panel">
          <style>wxTAB_TRAVERSAL</style>
          <object class="wxFlexGridSizer" name="">
            <vgap>5</vgap>
            <hgap>5</hgap>
            <growablerows></growablerows>
            <growablecols>1</growablecols>
            <rows>4</rows>
            <cols>2</cols>
            <object class="sizeritem">
              <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
              <border>3</border>
              <option>0</option>
              <object class="wxStaticText" name="name_label">
                <label>Name:</label>
                <wrap>0</wrap>
              </object>
            </object>
            <object class="sizeritem">
              <flag>wxEXPAND|wxALL</flag>
              <border>3</border>
              <option>0</option>
              <object class="wxTextCtrl" name="name_input">
                <value>Enter your name</value>
                <maxlength>0</maxlength>
              </object>
            </object>
            <object class="sizeritem">
              <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
              <border>3</border>
              <option>0</option>
              <object class="wxStaticText" name="email_label">
                <label>Email:</label>
                <wrap>0</wrap>
              </object>
            </object>
            <object class="sizeritem">
              <flag>wxEXPAND|wxALL</flag>
              <border>3</border>
              <option>0</option>
              <object class="wxTextCtrl" name="email_input">
                <value>your.email@example.com</value>
                <maxlength>0</maxlength>
              </object>
            </object>
            <object class="sizeritem">
              <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
              <border>3</border>
              <option>0</option>
              <object class="wxStaticText" name="category_label">
                <label>Category:</label>
                <wrap>0</wrap>
              </object>
            </object>
            <object class="sizeritem">
              <flag>wxEXPAND|wxALL</flag>
              <border>3</border>
              <option>0</option>
              <object class="wxChoice" name="category_choice">
                <selection>0</selection>
                <content>
                  <item>General</item>
                  <item>Technical</item>
                  <item>Feedback</item>
                  <item>Bug Report</item>
                </content>
              </object>
            </object>
            <object class="sizeritem">
              <flag>wxALIGN_TOP|wxALL</flag>
              <border>3</border>
              <option>0</option>
              <object class="wxStaticText" name="options_label">
                <label>Options:</label>
                <wrap>0</wrap>
              </object>
            </object>
            <object class="sizeritem">
              <flag>wxEXPAND|wxALL</flag>
              <border>3</border>
              <option>0</option>
              <object class="wxBoxSizer" name="">
                <orient>wxVERTICAL</orient>
                <object class="sizeritem">
                  <flag>wxEXPAND|wxALL</flag>
                  <border>2</border>
                  <option>0</option>
                  <object class="wxCheckBox" name="newsletter_check">
                    <label>Subscribe to newsletter</label>
                    <checked>0</checked>
                  </object>
                </object>
                <object class="sizeritem">
                  <flag>wxEXPAND|wxALL</flag>
                  <border>2</border>
                  <option>0</option>
                  <object class="wxCheckBox" name="updates_check">
                    <label>Receive updates</label>
                    <checked>1</checked>
                  </object>
                </object>
              </object>
            </object>
          </object>
        </object>
      </object>
      <object class="sizeritem">
        <flag>wxEXPAND|wxALL</flag>
        <border>10</border>
        <option>0</option>
        <object class="wxStaticText" name="dialog_message">
          <style>wxST_ELLIPSIZE_END</style>
          <label>This dialog was loaded from XRC and demonstrates various controls.</label>
          <wrap>0</wrap>
        </object>
      </object>
      <object class="sizeritem">
        <flag>wxEXPAND|wxALL</flag>
        <border>10</border>
        <option>0</option>
        <object class="wxBoxSizer" name="">
          <orient>wxHORIZONTAL</orient>
          <object class="spacer">
            <flag>wxEXPAND</flag>
            <border>5</border>
            <option>1</option>
            <size>0,0</size>
          </object>
          <object class="sizeritem">
            <flag>wxALL</flag>
            <border>5</border>
            <option>0</option>
            <object class="wxButton" name="dialog_ok_btn">
              <label>OK</label>
              <default>1</default>
              <auth_needed>0</auth_needed>
              <markup>0</markup>
              <bitmap/>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALL</flag>
            <border>5</border>
            <option>0</option>
            <object class="wxButton" name="dialog_cancel_btn">
              <label>Cancel</label>
              <default>0</default>
              <auth_needed>0</auth_needed>
              <markup>0</markup>
              <bitmap/>
            </object>
          </object>
        </object>
      </object>
    </object>
  </object>
</resource>
