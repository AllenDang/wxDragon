<?xml version="1.0" encoding="UTF-8"?>
<resource xmlns="http://www.wxwidgets.org/wxxrc" version="2.5.3.0">
  <object class="wxPanel" name="test_panel">
    <style>wxTAB_TRAVERSAL|wxBORDER_SIMPLE</style>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <flag>wxEXPAND|wxALL</flag>
        <border>10</border>
        <object class="wxStaticText" name="test_label">
          <label>XRC Test Panel</label>
        </object>
      </object>
      <object class="sizeritem">
        <flag>wxEXPAND|wxALL</flag>
        <border>10</border>
        <object class="wxButton" name="test_button">
          <label>Test Button</label>
        </object>
      </object>
    </object>
  </object>
  
  <!-- Test panel with BORDER_THEME -->
  <object class="wxPanel" name="theme_panel">
    <style>wxTAB_TRAVERSAL|wxBORDER_THEME</style>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <flag>wxEXPAND|wxALL</flag>
        <border>10</border>
        <object class="wxStaticText" name="theme_label">
          <label>Theme Border Panel</label>
        </object>
      </object>
    </object>
  </object>
</resource> 